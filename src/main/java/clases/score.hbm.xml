<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="clases">
	<class name="Score" table="SCORE">
		<id name="score_id" column="USER_ID" type="long">
			<generator class="native" />
		</id>
		<property name="score_comment" type="string" />
		<property name="score_number" type="int" />

	<!-- 		
		<many-to-one name="score_trip" column="trip_id"></many-to-one>
	 -->	
	</class>

</hibernate-mapping>
