<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="clases">
	<class name="Trip" table="TRIP">
		<id name="trip_id" column="TRIP_ID" type="long">
			<generator class="native" />
		</id>
		<property name="trip_cost" type="int" />
		<property name="trip_numberOfpassengers" type="int" />
		<property name="trip_date" type="date" />
		<property name="trip_from" type="string" />
		<property name="trip_to" type="string" />
		<!--  <property name="trip_passengers" /> -->
	</class>
</hibernate-mapping>