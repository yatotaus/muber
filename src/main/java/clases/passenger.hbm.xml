<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="clases">

	<class name="Passenger" table="USER">
		<id name="user_id" column="USER_ID" type="long">
			<generator class="native" />
		</id>
		<property name="user_name" not-null="true" type="string" />
		<property name="user_password" type="string" />
		<property name="user_date" type="date" />
		<property name="passenger_credits" type="long" not-null="true"/>
		
	</class>

</hibernate-mapping>